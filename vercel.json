{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/css/main.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/media/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/public/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
