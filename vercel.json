{
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "/public/media/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    }
  ],
  "headers": [
    {
      "source": "/media/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "video/mp4" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "image/png" }
      ]
    }
  ]
}
